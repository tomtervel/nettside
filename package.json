{
  "name": "tomtervel",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "start": "bankai start",
    "build": "bankai build index.js ./dist",
    "test": "standard && npm run deps && node test.js"
  },
  "main": "index.js",
  "engine": {
    "node": "6.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/tomtervel/nettside.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bankai": "^9.15.0",
    "brfs": "^1.4.3",
    "choo": "^6.13.1",
    "choo-devtools": "^2.5.1",
    "choo-persist": "^3.0.2",
    "choo-service-worker": "^2.4.0",
    "dotenv": "^5.0.0",
    "envify": "^4.1.0",
    "hstream": "^1.2.0",
    "markdown-it": "^8.4.2",
    "nanocomponent": "^6.4.2",
    "nanocontent": "^0.4.6",
    "nanopage": "0.0.2",
    "on-idle": "^3.1.4",
    "sheetify": "^7.3.3",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "dependency-check": "^3.2.1",
    "minify-stream": "1.1.*",
    "standard": "^11.0.1"
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "browserify": {
    "transform": [
      "envify",
      "brfs",
      "nanocontent/transform"
    ]
  },
  "documentify": {
    "transform": [
      "./headertags.js"
    ]
  }
}
