{
  "name": "tomtervel",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "start": "bankai start",
    "build": "bankai build; npm run build:404",
    "build:404": "create-html --head '<script>sessionStorage.redirect = location.href;</script><meta http-equiv=\"refresh\" content=\"0;URL='/'\">' > dist/404.html",
    "test": "standard && npm run deps && node test.js"
  },
  "main": "index.js",
  "engine": {
    "node": "6.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/tomtervel/nettside.git"
  },
  "dependencies": {
    "bankai": "^9.0.0-rc6",
    "brfs": "^1.4.3",
    "choo": "^6.4.2",
    "choo-devtools": "^1.4.0",
    "choo-persist": "^3.0.2",
    "create-html": "^4.0.0",
    "dotenv": "^4.0.0",
    "envify": "^4.1.0",
    "marked": "^0.3.6",
    "nanocomponent": "^6.4.2",
    "sheetify": "^6.2.0",
    "tachyons": "^4.8.1"
  },
  "devDependencies": {
    "choo-log": "^7.2.1",
    "dependency-check": "^2.9.1",
    "standard": "^10.0.3"
  },
  "browserify": {
    "transform": [
      "brfs",
      "envify"
    ]
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
